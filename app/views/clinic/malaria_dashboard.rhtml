    <%= javascript_include_tag("jquery.min") %>
    <style>
      .navbar-brand {
        font-size: 26px;
        letter-spacing: 2px;
        text-shadow: 1px 1px 5px rgba(0, 0, 0, 0.5);
      }

      .navbar-inner {
        height: 52px;
        padding-bottom: 0;
        padding-top: 0;
      }

