                var ex_amount = __$(drug + "_expire_amount_" + i).value;
                var date = __$(drug + "_date_" + i).value;

                cleanParams(drug, i);
                buildParams(drug, (i - 1));
                feedParams(drug, (i - 1), [amount, ex_amount, date]);
